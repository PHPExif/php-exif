{
    "name": "miljar/php-exif",
    "description": "Object-Oriented EXIF parsing",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Van Herreweghe",
            "homepage": "http://theanalogguy.be",
            "role": "Developer"
        }
    ],
    "keywords": ["EXIF", "IPTC", "jpeg", "tiff", "exiftool"],
    "require": {
        "php": "^5.6 || ^7.0"
    },
    "require-dev": {
        "phpmd/phpmd": "~2.3.0",
        "squizlabs/php_codesniffer": "~2.0",
        "phpunit/phpunit": "~5.2.0",
        "satooshi/php-coveralls": "~1.0.0",
        "mockery/mockery": "^0.9.4"
    },
    "suggest": {
        "lib-exiftool": "Use perl lib exiftool as adapter"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": { "PHPExif\\": "src/PHPExif/" }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\PHPExif\\": "tests/PHPExif/" }
    }
}
